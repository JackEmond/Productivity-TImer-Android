<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RV_Items" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/bebas_neue</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.25</item>
    </style>

    <style name="Timer_Buttons" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/bebas_neue</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">22dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:textColor">#fff</item>
        <item name="colorAccent">@color/black</item>
        <item name="background">@color/black</item>
    </style>

    <style name="BottomoNavigationView.Active" parent="TextAppearance.AppCompat">
        <item name="fontFamily">@font/bebas_neue</item>
    </style>

    <style name="BottomoNavigationView.Inactive" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="fontFamily">@font/bebas_neue</item>
    </style>
</resources>

